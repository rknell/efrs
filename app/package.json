{
  "name": "checkforms",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "postinstall": "npx jetify",
    "testwatch": "jest --watch",
    "build:ios": "react-native bundle --entry-file='index.ios.js' --bundle-output='./ios/checkforms/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "fix:vectoricons": "rm node_modules/react-native/local-cli/core/__fixtures__/files/package.json",
    "codepush:ios": "if [ -z \\\"$CODEPUSH_PRIVATE_KEY_PATH\\\" ]; then echo \\\"Set CODEPUSH_PRIVATE_KEY_PATH before running this script\\\" && exit 1; fi; code-push release-react efrs-ios ios -k \\\"$CODEPUSH_PRIVATE_KEY_PATH\\\"",
    "codepush:android": "if [ -z \\\"$CODEPUSH_PRIVATE_KEY_PATH\\\" ]; then echo \\\"Set CODEPUSH_PRIVATE_KEY_PATH before running this script\\\" && exit 1; fi; code-push release-react efrs-android android -k \\\"$CODEPUSH_PRIVATE_KEY_PATH\\\"",
    "codepush:deploy": "git status --porcelain && yarn test && yarn codepush:ios && yarn codepush:android",
    "debug:android:dependencies": "cd android && ./gradlew -q dependencies app:dependencies --configuration compile"
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "blueimp-md5": "^2.10.0",
    "bson-objectid": "^1.2.4",
    "bugsnag-react-native": "^2.23.2",
    "debounce": "^1.2.0",
    "email-validator": "^2.0.4",
    "fbjs": "^1.0.0",
    "fetch-mock": "^7.3.0",
    "jetifier": "^1.6.5",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "lz-string": "^1.4.4",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "prop-types": "^15.6.2",
    "react": "16.8.3",
    "react-dom": "^16.7.0",
    "react-native": "0.59.10",
    "react-native-background-upload": "^5.7.0",
    "react-native-calendars": "https://github.com/rknell/react-native-calendars.git",
    "react-native-camera": "^3.29.0",
    "react-native-camera-roll-picker": "^1.2.3",
    "react-native-circular-action-menu": "https://github.com/rknell/react-native-circular-action-menu",
    "react-native-code-push": "^5.6.0",
    "react-native-deprecated-custom-components": "https://github.com/facebookarchive/react-native-custom-components.git",
